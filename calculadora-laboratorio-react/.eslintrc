{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
        "browser": true
    },
    "rules": {
        "semi": ["error", "never"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/extensions": [0,"always"],
        "react/prop-types": 0,
        "linebreak-style": 0,
        "global-require": 0,
        "eslint linebreak-style": [0, "error", "windows"],
        "indent": "off",
        "react/jsx-indent": "off",
        "react/jsx-indent-props": "off",
        "no-tabs": 0
    }
}